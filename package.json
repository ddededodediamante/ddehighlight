{
  "name": "ddehighlight",
  "displayName": "ddeHighlight",
  "description": "Adds syntax highlighting for my custom language ddeLang.",
  "publisher": "ddededodediamante",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "dde",
        "aliases": [
          "ddeLang",
          "ddeInterpreter",
          "dde"
        ],
        "extensions": [
          ".dde"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./images/text.png",
          "dark": "./images/text.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "dde",
        "scopeName": "source.dde",
        "path": "./syntaxes/dde.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "dde",
        "path": "./snippets/dde.code-snippets"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ddededodediamante/ddehighlight"
  },
  "main": "./extension.js"
}
