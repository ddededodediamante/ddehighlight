{
  "name": "ddelang",
  "displayName": "ddeLang",
  "description": "Adds support for the custom .dde language, with autocomplete, highlighting and more.",
  "publisher": "ddededodediamante",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "dde",
        "aliases": [
          "ddeLang",
          "ddeInterpreter",
          "dde",
          "ddededodediamante"
        ],
        "extensions": [
          ".dde"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./images/text.png",
          "dark": "./images/text.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "dde",
        "scopeName": "source.dde",
        "path": "./syntaxes/dde.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "dde",
        "path": "./snippets/dde.code-snippets"
      }
    ],
    "commands": [
      {
        "command": "dde.runFile",
        "title": "Run DDE File"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "dde.runFile",
          "when": "resourceExtname == .dde",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "command": "dde.runFile",
          "when": "resourceExtname == .dde"
        }
      ]
    }
  },
  "activationEvents": [
    "onLanguage:dde",
    "onCommand:dde.runFile"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ddededodediamante/ddehighlight"
  },
  "main": "extension.js"
}
